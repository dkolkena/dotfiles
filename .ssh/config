  User dkolkena
  #ProxyCommand "s rock401.eigdev.com"

Host zugzug
  HostName zugzug.bluehost.com
  User dkolkena
  Port 5190
  ForwardAgent yes


#### Global defaults

Host *
  # Default user
  User dkolkena
  
  # Auth settings 
  IgnoreUnknown AddKeysToAgent,UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
  
  # Connection settings
  ForwardAgent no
  ForwardX11 no
  ForwardX11Trusted yes
  Port 22
  Protocol 2
  ServerAliveInterval 60
  ServerAliveCountMax 5
  




